{
    "arch": [
        "amd64"
    ],
    "auth_api": false,
    "auto_uart": false,
    "boot": "auto",
    "description": "Homeassitant add-on for frigate!",
    "devices": [
        "/dev/bus/usb:/dev/bus/usb:rwm",
        "/dev/dri/renderD128:/dev/dri/renderD128:rwm"
    ],
    "full_access": true,
    "hassio_api": false,
    "hassio_role": "default",
    "homeassistant_api": false,
    "host_network": true,
    "ingress": false,
    "ingress_port": 0,
    "map": [
        "config:rw"
    ],
    "name": "Frigate",
    "options": {
        "CONFIG_FILE": "/config/frigate/config.yml",
        "FRIGATE_RTSP_PASSWORD": "yourpassword"
    },
    "panel_icon": "mdi:chip",
    "ports": {
        "5000/tcp": 5000
    },
    "ports_description": {
        "5000/tcp": "Web interface (Not required for Hass.io Ingress)"
    },
    "schema": {
        "CONFIG_FILE": "str",
        "FRIGATE_RTSP_PASSWORD": "str"
    },
    "slug": "frigate",
    "startup": "application",
    "url": "https://github.com/mories76/ha-addon-frigate",
    "version": "0.0.2",
    "webui": "http://[HOST]:[PORT:5000]/debug/stats"
}